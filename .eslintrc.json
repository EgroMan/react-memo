{
  "extends": ["eslint:recommended", "prettier", "react-app", "react-app/jest"],
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": "off",
    "no-undef": "off",
    "camelcase": ["error", { "properties": "never" }],
    "eqeqeq": ["error", "always"],
    "no-unused-expressions": "off"  // Добавляем правило "no-unused-expressions" и отключаем его
  }
}
